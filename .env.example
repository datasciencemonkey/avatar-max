# AI Provider Configuration
# Options: "fal" or "replicate"
AI_PROVIDER=replicate

# Fal AI Configuration
# Get your API key from: https://fal.ai/dashboard/keys
FAL_KEY=your_fal_api_key_here

# Replicate Configuration
# Get your API token from: https://replicate.com/account/api-tokens
REPLICATE_API_TOKEN=your_replicate_api_token_here

# Databricks Configuration (for Claude commentary)
DATABRICKS_TOKEN=your_databricks_token_here
DATABRICKS_CLAUDE_ENDPOINT=you_host

# Event Configuration (optional)
EVENT_NAME=Databricks @ Innovation Garage 2025


# Databricks Volume (optional)
DATABRICKS_VOLUME=/Volumes/main/sgfs/sg-vol/avatarmax

# Brevo SMTP Configuration (for email delivery)
# Get your SMTP key from: https://app.brevo.com/settings/keys/smtp
BREVO_SMTP_SERVER=smtp-relay.brevo.com
BREVO_SMTP_PORT=587
BREVO_SMTP_LOGIN=93330f001@smtp-brevo.com
BREVO_SMTP_PASSWORD=your_brevo_smtp_password_here

# Email Configuration
EMAIL_FROM_ADDRESS=avatars@yourdomain.com
EMAIL_FROM_NAME=Innovation Garage Superhero Creator
EMAIL_REPLY_TO=noreply@yourdomain.com
EMAIL_BATCH_SIZE=50
EMAIL_MAX_RETRIES=3
ENABLE_EMAIL_FEATURE=true

# PostgreSQL Database Configuration
DATABASE_URL=postgresql://user:password@localhost:5432/superhero_avatars
# Or use individual components:
# DB_HOST=localhost
# DB_PORT=5432
# DB_NAME=superhero_avatars
# DB_USER=postgres
# DB_PASSWORD=your_password